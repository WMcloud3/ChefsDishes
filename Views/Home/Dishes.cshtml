<h1><a href="/">Chefs</a> | Dishes</h1>

<a href="/dishes/new">Add a Dish</a>

<div id="container">
    <table class="table table-dark table-hover">
        <tr>
            <th>Name</th>
            <th>Chef</th>
            <th>Tastiness</th>
            <th>Calories</th>
        </tr>
        @foreach (Dish dish in ViewBag.AllDishes)
        {
            <tr>
                <td>@dish.DishName</td>
                <td>@dish.Creator.FirstName @dish.Creator.LastName</td>
                <td>@dish.Taste</td>
                <td>@dish.Calories</td>
            </tr>
        }
    </table>
</div>